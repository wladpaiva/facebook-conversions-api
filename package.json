{
  "name": "next-facebook-tracking",
  "description": "A simple package to help you integrate Facebook Pixel and Conversions API with Next.js",
  "version": "0.1.7-alpha.6",
  "keywords": [
    "next",
    "facebook",
    "tracking",
    "pixel",
    "conversions",
    "api"
  ],
  "files": [
    "dist"
  ],
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.cjs",
      "types": "./dist/components/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs",
      "types": "./dist/utils.d.ts"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/facebook-nodejs-business-sdk": "^20.0.2",
    "@types/parse-full-name": "^1.2.5",
    "@types/react": "^18.3.3",
    "tsup": "^8.2.3"
  },
  "peerDependencies": {
    "next": "^14",
    "react": "^18",
    "typescript": "^5"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.0",
    "facebook-nodejs-business-sdk": "^20.0.0",
    "parse-full-name": "^1.2.6",
    "uuidv7": "^1.0.1"
  }
}
